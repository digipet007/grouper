<!-- Group Name drop down -->
<form action="GET">
    <div class="input-field col s12">
        <select id="dropdown-home">
            <option value="" selected>Not Selected</option>
                {{#each groups}}
                    <option value="{{this.group_}}">{{this.group_}}</option>
                {{/each}}
            </select>
        <label>Pick an Existing Group</label>
    </div>
</form>

